{
  "name": "@honeylemon/contracts",
  "version": "0.1.0",
  "description": "Solidity Contracts for Honeylemon",
  "main": "lib/src/index.js",
  "scripts": {
    "build": "npx truffle compile && yarn generate-types && npx tsc -d --declarationDir lib --declarationMap",
    "generate-types": "yarn generate-types:truffle && yarn generate-types:web3 && yarn generate-types:ethers",
    "generate-types:truffle": "npx typechain \"build/contracts/*.json\" --target=truffle-v5 --outDir lib/types/truffle",
    "generate-types:web3": "npx typechain \"build/contracts/*.json\" --target=web3-v1 --outDir lib/types/web3",
    "generate-types:ethers": "npx typechain \"build/contracts/*.json\" --target=ethers-v4 --outDir lib/types/ethers",
    "migrate": "tsc -p ./tsconfig.migrate.json --outDir ./migrations && truffle migrate",
    "test": "truffle test test/honeylemon/*",
    "lint": "prettier-eslint --write \"$(pwd)/**/*.js\" \"$(pwd)/**/*.sol\" --ignore './node_modules/**'",
    "postinstall": "yarn build",
    "remixd": "remixd -s . --remix-ide https://remix.ethereum.org"
  },
  "author": "@carboclan",
  "license": "Apache-2.0",
  "dependencies": {
    "@openzeppelin/test-helpers": "^0.5.6",
    "@truffle/hdwallet-provider": "^1.0.35",
    "@typechain/ethers-v4": "1.0.0",
    "@typechain/truffle-v5": "2.0.2",
    "@typechain/web3-v1": "1.0.0",
    "assert": "^2.0.0",
    "bn.js": "^4.11.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-cli": "^1.1.1",
    "ethers": "^4.0.0",
    "ganache-time-traveler": "^1.0.14",
    "openzeppelin-solidity": "2.2.0",
    "pg": "^8.2.1",
    "prettier-eslint": "^9.0.1",
    "prettier-eslint-cli": "5.0.0",
    "prettier-plugin-solidity": "^1.0.0-alpha.47",
    "solidity-coverage": "0.6.7",
    "solidoc": "^1.0.5",
    "solium": "^1.0.5",
    "truffle": "5.1.18",
    "truffle-assertions": "0.9.0",
    "truffle-flattener": "^1.4.4",
    "truffle-typings": "1.0.8",
    "typechain": "^2.0.0",
    "typescript": "3.9.5",
    "web3": "1.2.9",
    "web3-core": "^1.2.7",
    "web3-core-helpers": "^1.2.7",
    "web3-core-method": "^1.2.7",
    "web3-utils": "^1.2.7"
  },
  "devDependencies": {}
}
