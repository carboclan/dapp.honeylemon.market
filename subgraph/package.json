{
  "name": "honeylemon-graph",
  "version": "0.1.0",
  "scripts": {
    "create": "graph create andrei-anisimov/honeylemon --node https://api.thegraph.com/deploy/",
    "create-local": "graph create honeylemon/honeylemon-graph --node http://127.0.0.1:8020",
    "codegen": "graph codegen --debug --output-dir src/types/",
    "build": "graph build --debug",
    "deploy": "graph deploy andrei-anisimov/honeylemon --debug --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy honeylemon/honeylemon-graph --debug --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "prepare:kovan": "mustache config/kovan.json subgraph.template.yaml > subgraph.yaml",
    "prepare:local": "mustache config/local.json subgraph.template.yaml > subgraph.yaml"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.18.0",
    "@graphprotocol/graph-ts": "^0.18.0",
    "mustache": "^4.0.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "truffle": "^5.0.4",
    "truffle-contract": "^4.0.5",
    "truffle-hdwallet-provider": "^1.0.4"
  }
}
